  "components": {
    "schemas": {
      "ContactUs": {
        "required": [
          "email",
          "message",
          "name"
        ],
        "type": "object",
        "properties": {
          "id": {
            "type": "integer",
            "format": "int32"
          },
          "name": {
            "maxLength": 100,
            "minLength": 0,
            "type": "string"
          },
          "email": {
            "maxLength": 100,
            "minLength": 0,
            "type": "string",
            "format": "email"
          },
          "message": {
            "minLength": 1,
            "type": "string"
          },
          "isRead": {
            "type": "boolean"
          },
          "createdAt": {
            "type": "string",
            "format": "date-time"
          }
        },
        "additionalProperties": false
      },
      "CreateFeedbackDto": {
        "required": [
          "message"
        ],
        "type": "object",
        "properties": {
          "message": {
            "minLength": 1,
            "type": "string"
          },
          "rating": {
            "maximum": 5,
            "minimum": 1,
            "type": "integer",
            "format": "int32"
          }
        },
        "additionalProperties": false
      },
      "CreatePaymentDto": {
        "required": [
          "amount",
          "paymentMethod",
          "userId"
        ],
        "type": "object",
        "properties": {
          "userId": {
            "type": "integer",
            "format": "int32"
          },
          "amount": {
            "minimum": 0,
            "type": "number",
            "format": "double"
          },
          "paymentMethod": {
            "maxLength": 50,
            "minLength": 0,
            "type": "string"
          },
          "notes": {
            "type": "string",
            "nullable": true
          }
        },
        "additionalProperties": false
      },
      "Equipment": {
        "required": [
          "roomId"
        ],
        "type": "object",
        "properties": {
          "id": {
            "type": "integer",
            "format": "int32"
          },
          "name": {
            "type": "string",
            "nullable": true
          },
          "quantity": {
            "type": "integer",
            "format": "int32"
          },
          "importDate": {
            "type": "string",
            "format": "date-time"
          },
          "warranty": {
            "type": "string",
            "format": "date-time"
          },
          "status": {
            "type": "string",
            "nullable": true
          },
          "roomId": {
            "type": "integer",
            "format": "int32"
          },
          "gymRoom": {
            "$ref": "#/components/schemas/GymRoom"
          },
          "equipmentMaintenances": {
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/EquipmentMaintenance"
            },
            "nullable": true
          }
        },
        "additionalProperties": false
      },
      "EquipmentMaintenance": {
        "required": [
          "equipmentId"
        ],
        "type": "object",
        "properties": {
          "id": {
            "type": "integer",
            "format": "int32"
          },
          "date": {
            "type": "string",
            "format": "date-time"
          },
          "issueDescription": {
            "type": "string",
            "nullable": true
          },
          "status": {
            "type": "string",
            "nullable": true
          },
          "equipmentId": {
            "type": "integer",
            "format": "int32"
          },
          "equipment": {
            "$ref": "#/components/schemas/Equipment"
          }
        },
        "additionalProperties": false
      },
      "Feedback": {
        "required": [
          "message",
          "userId"
        ],
        "type": "object",
        "properties": {
          "id": {
            "type": "integer",
            "format": "int32"
          },
          "userId": {
            "type": "integer",
            "format": "int32"
          },
          "message": {
            "minLength": 1,
            "type": "string"
          },
          "rating": {
            "maximum": 5,
            "minimum": 1,
            "type": "integer",
            "format": "int32"
          },
          "createdAt": {
            "type": "string",
            "format": "date-time"
          }
        },
        "additionalProperties": false
      },
      "GymRoom": {
        "type": "object",
        "properties": {
          "id": {
            "type": "integer",
            "format": "int32"
          },
          "roomName": {
            "type": "string",
            "nullable": true
          },
          "roomType": {
            "type": "string",
            "nullable": true
          },
          "roomQuantity": {
            "type": "integer",
            "format": "int32"
          },
          "roomStatus": {
            "type": "string",
            "nullable": true
          },
          "trainningSessions": {
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/TrainningSession"
            },
            "nullable": true
          },
          "equipments": {
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/Equipment"
            },
            "nullable": true
          }
        },
        "additionalProperties": false
      },
      "LoginRequest": {
        "type": "object",
        "properties": {
          "email": {
            "type": "string",
            "nullable": true
          },
          "password": {
            "type": "string",
            "nullable": true
          }
        },
        "additionalProperties": false
      },
      "Payment": {
        "required": [
          "amount",
          "paymentDate",
          "paymentMethod",
          "status",
          "userId"
        ],
        "type": "object",
        "properties": {
          "id": {
            "type": "integer",
            "format": "int32"
          },
          "userId": {
            "type": "integer",
            "format": "int32"
          },
          "amount": {
            "minimum": 0,
            "type": "number",
            "format": "double"
          },
          "paymentMethod": {
            "maxLength": 50,
            "minLength": 0,
            "type": "string"
          },
          "paymentDate": {
            "type": "string",
            "format": "date-time"
          },
          "status": {
            "maxLength": 50,
            "minLength": 0,
            "type": "string"
          },
          "transactionId": {
            "type": "string",
            "nullable": true
          },
          "notes": {
            "maxLength": 500,
            "minLength": 0,
            "type": "string",
            "nullable": true
          },
          "user": {
            "$ref": "#/components/schemas/User"
          },
          "subscription": {
            "$ref": "#/components/schemas/Subscription"
          }
        },
        "additionalProperties": false
      },
      "Plan": {
        "required": [
          "description",
          "durationInDays",
          "imageUrl",
          "name",
          "price"
        ],
        "type": "object",
        "properties": {
          "id": {
            "type": "integer",
            "format": "int32"
          },
          "name": {
            "maxLength": 100,
            "minLength": 0,
            "type": "string"
          },
          "description": {
            "maxLength": 1000,
            "minLength": 0,
            "type": "string"
          },
          "price": {
            "minimum": 0,
            "type": "number",
            "format": "double"
          },
          "durationInDays": {
            "maximum": 3650,
            "minimum": 1,
            "type": "integer",
            "format": "int32"
          },
          "isActive": {
            "type": "boolean"
          },
          "createdAt": {
            "type": "string",
            "format": "date-time"
